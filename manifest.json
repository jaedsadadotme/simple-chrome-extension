{
    "manifest_version": 2,
    "name": "Jaedsada.me",
    "version": "0.1.0",

    "permissions": ["<all_urls>"],
    "icons": {
      "128": "logo.png"
    },
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "logo.png"
    },
    "content_security_policy": "script-src 'self' https://unpkg.com ; object-src 'self'"
  }